<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>capstonewar.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>capstonewar.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>capstonewar.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$PurchaseOrder</managed-bean-name>
        <managed-bean-class>capstonewar.pages.PurchaseOrder</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$PurchaseOrderCreate</managed-bean-name>
        <managed-bean-class>capstonewar.pages.PurchaseOrderCreate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/PurchaseOrder.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/PurchaseOrderCreate.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/ReceiveDelivery.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/PurchaseOrderCreate.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/PurchaseOrder.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/ReceiveDelivery.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ReceiveDelivery</managed-bean-name>
        <managed-bean-class>capstonewar.pages.ReceiveDelivery</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ReceiveDelivery.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/PurchaseOrder.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/ReceiveDeliveryCreate.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ReceiveDeliveryCreate</managed-bean-name>
        <managed-bean-class>capstonewar.pages.ReceiveDeliveryCreate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ReceiveDeliveryCreate.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/ReceiveDelivery.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/PurchaseOrder.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$SupplierMaintenance</managed-bean-name>
        <managed-bean-class>capstonewar.pages.SupplierMaintenance</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$SupplierMaintenanceCreate</managed-bean-name>
        <managed-bean-class>capstonewar.pages.SupplierMaintenanceCreate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/SupplierMaintenance.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/SupplierMaintenanceCreate.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/ReceiveDelivery.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case3</from-outcome>
            <to-view-id>/pages/PurchaseOrder.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case4</from-outcome>
            <to-view-id>/pages/SupplierMaintenanceUpdate.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case5</from-outcome>
            <to-view-id>/pages/ProductUnitMaintenance.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/SupplierMaintenanceCreate.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/SupplierMaintenance.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/SupplierMaintenance.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$SupplierMaintenanceUpdate</managed-bean-name>
        <managed-bean-class>capstonewar.pages.SupplierMaintenanceUpdate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/SupplierMaintenanceUpdate.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/SupplierMaintenance.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$navigator</managed-bean-name>
        <managed-bean-class>capstonewar.pages.navigator</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$ProductUnitMaintenance</managed-bean-name>
        <managed-bean-class>capstonewar.pages.ProductUnitMaintenance</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ProductUnitMaintenance.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/SupplierMaintenance.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/ProductUnitCreate.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case3</from-outcome>
            <to-view-id>/pages/ProductUnitUpdate.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ProductUnitCreate</managed-bean-name>
        <managed-bean-class>capstonewar.pages.ProductUnitCreate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ProductUnitCreate.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/ProductUnitMaintenance.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ProductUnitUpdate</managed-bean-name>
        <managed-bean-class>capstonewar.pages.ProductUnitUpdate</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ProductUnitUpdate.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/ProductUnitMaintenance.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ProductVarianceMaintenance</managed-bean-name>
        <managed-bean-class>capstonewar.pages.ProductVarianceMaintenance</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ProductVarianceMaintenance.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/pages/SupplierMaintenance.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/pages/ProductUnitMaintenance.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
